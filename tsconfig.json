{
  "include": ["src"],
  "exclude": ["node_modules"], 
  "compilerOptions": {
    "target": "ES2015",
    "removeComments": true,
    "baseUrl": "src",
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    // "noImplicitAny": false, 
    "forceConsistentCasingInFileNames": true
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
